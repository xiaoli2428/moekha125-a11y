# Production Environment Variables
# ================================
# Add your production API keys and configuration here

# App Configuration
VITE_APP_NAME=OnchainWeb
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=production

# API Endpoints (replace with your actual endpoints)
# VITE_API_URL=https://api.onchainweb.co
# VITE_WS_URL=wss://ws.onchainweb.co

# Web3 Configuration (replace with your actual values)
# VITE_CHAIN_ID=1
# VITE_RPC_URL=https://mainnet.infura.io/v3/YOUR_PROJECT_ID
# VITE_WALLET_CONNECT_PROJECT_ID=YOUR_WALLET_CONNECT_PROJECT_ID

# Analytics (optional)
# VITE_GA_TRACKING_ID=G-XXXXXXXXXX
# VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
