{
  "buildCommand": "npm run build",
  "installCommand": "npm install --no-save",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/|_next/static|_next/image|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}